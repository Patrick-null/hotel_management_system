<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.patrick.mapper.MemberMapper">
    <insert id="insert">
        INSERT INTO `member`(`mname`,`mgender`,`mno`,`mphone`) VALUES (#{mname},#{mgender},#{mno},#{mphone})
    </insert>
    <update id="update">
        update `member` set `mname`=#{mname},`mgender`=#{mgender},`mno`=#{mno},`mphone`=#{mphone} where `mid`=#{mid}
    </update>
    <delete id="delete">
        delete from `member` where `mid`=#{mid}
    </delete>
    <select id="selectAll" resultType="member">
        select * from `member`
    </select>
    <select id="selectById" resultType="member">
        select * from `member` where `mid`=#{mid}
    </select>
</mapper>